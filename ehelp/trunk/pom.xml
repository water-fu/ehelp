  <version>0.01-SNAPSHOT</version>
  <name>elink</name>
  <url>http://localhost:8080/</url>


  <properties>
    <!-- spring版本号 -->
    <spring.version>3.2.4.RELEASE</spring.version>
    <!-- mybatis版本号 -->
